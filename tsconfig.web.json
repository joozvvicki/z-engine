{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/engine/**/*.*",
    "src/ui/src/**/*",
    "src/ui/src/**/*.vue",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@ui/*": [
        "src/ui/src/*"
      ],
      "@engine/*": [
        "src/engine/*"
      ]
    }
  }
}
